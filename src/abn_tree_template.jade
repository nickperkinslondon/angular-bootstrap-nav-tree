ul.nav.nav-list.nav-pills.nav-stacked.abn-tree
  li.abn-tree-row(ng-repeat='row in tree_rows | filter:{visible:true} track by row.branch.uid', ng-animate="'abn-tree-animate'", style='margin:0', ng-class="'level-' + {{ row.level }} + (row.branch.selected ? 'active':'')")
    a(style='width:100%;padding-right: {{20* row.level}}px;')
      i.(ng-class='row.tree_icon', ng-click='row.branch.expanded = !row.branch.expanded;onExpand(row)', class='\"indented')  
      span.indented.tree-label(ng-click='user_clicks_branch(row.branch)', style='font-size: 11px;') {{ row.label }} 
