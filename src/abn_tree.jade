ul.nav.nav-list.abn-tree

  li.nav-header(data-ng-show="header") {{ header }}

  li.abn-tree-row(
    data-ng-repeat='row in tree_rows | filter:{visible:true} track by row.branch.uid'
    data-ng-animate="'abn-tree-animate'"
    data-ng-class="'level-' + {{ row.level }} + (row.branch.selected ? ' active':'')"
    )

    a(data-ng-click="user_clicks_branch(row.branch)")

      i.indented.tree-icon(
        data-ng-class="row.tree_icon"
        data-ng-click="row.branch.expanded = !row.branch.expanded"
        ) 

      span.indented.tree-label {{ row.label }}
